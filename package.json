{
  "name": "docdobackend",
  "version": "1.0.0",
  "description": "DocDoBackend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "./node_modules/.bin/lb-apidocs",
    "build": "./node_modules/.bin/lb-tsc es2017 --outDir dist",
    "build:watch": "./node_modules/.bin/lb-tsc --watch",
    "clean": "./node_modules/.bin/lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "./node_modules/.bin/lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "./node_modules/.bin/lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "./node_modules/.bin/lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "./node_modules/.bin/lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t docdobackend .",
    "docker:run": "docker run -p 3000:3000 -d docdobackend",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.0.17",
    "@loopback/boot": "^1.1.2",
    "@loopback/context": "^1.8.1",
    "@loopback/core": "^1.2.1",
    "@loopback/openapi-v3": "^1.3.2",
    "@loopback/repository": "^1.2.1",
    "@loopback/rest": "^1.9.1",
    "@loopback/rest-explorer": "^1.1.13",
    "@loopback/service-proxy": "^1.1.1",
    "loopback-cli": "^5.0.0",
    "loopback-connector-mongodb": "^4.0.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.4.2",
    "@loopback/testlab": "^1.2.1",
    "@loopback/tslint-config": "^2.0.3",
    "@types/node": "^10.11.2",
    "@types/passport": "^1.0.0",
    "@types/passport-http": "^0.3.7",
    "tslint": "^5.12.0",
    "typescript": "^3.3.1"
  }
}
